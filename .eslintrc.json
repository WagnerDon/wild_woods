{
 "env": {
  "browser": true
 },
 "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
 "parser": "@typescript-eslint/parser",
 "parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
 },
 "rules": {
  "quotes": ["error", "double"],
  "semi": ["error", "always"],
  "eol-last": ["error", "always"],
  "no-multiple-empty-lines": ["error", { "max": 1 }]
 },
 "plugins": ["@typescript-eslint"]
}
